if &compatible
  set nocompatible
end

filetype off
set rtp+=~/.vim/bundle/Vundle.vim
call vundle#rc()

" let vundle manage vundle
Bundle 'gmarik/vundle'

" repos on github
Bundle "tomasr/molokai"

" vim-scripts repos

" non github repos

if filereadable(expand("~/.vimrc.bundles.local"))
	source ~/.vimrc.bundles.local
endif

filetype on
